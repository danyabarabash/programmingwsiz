<Window x:Class="Zadanie_1_2.AddGrade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Zadanie_1_2"
        mc:Ignorable="d"
        Title="Add Grade" Height="168" Width="251">
    <Grid>
        <Label Content="Przedmiot" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label Content="Ocena" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox x:Name="tbPrzedmiot" HorizontalAlignment="Left" Margin="108,14,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <TextBox x:Name="tbOcena" HorizontalAlignment="Left" Margin="108,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <Button Content="Ok" HorizontalAlignment="Center" Margin="0,96,0,0" VerticalAlignment="Top" FontSize="20" Width="84" Click="Button_Click"/>

    </Grid>
</Window>
